

<h1>Ipv6 Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ipv6_addresses.each do |ipv6_address| %>
      <tr>
        <td><%= ipv6_address.address %></td>
        <td><%= link_to 'Show', ipv6_address %></td>
        <td><%= link_to 'Edit', edit_ipv6_address_path(ipv6_address) %></td>
        <td><%= link_to 'Destroy', ipv6_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ipv6 Address', new_ipv6_address_path %>
