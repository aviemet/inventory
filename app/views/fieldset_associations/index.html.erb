<p id="notice"><%= notice %></p>

<h1>Fieldset Associations</h1>

<table>
  <thead>
    <tr>
      <th>Fieldset</th>
      <th>Fieldable</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @fieldset_associations.each do |fieldset_association| %>
      <tr>
        <td><%= fieldset_association.fieldset_id %></td>
        <td><%= fieldset_association.fieldable_id %></td>
        <td><%= link_to 'Show', fieldset_association %></td>
        <td><%= link_to 'Edit', edit_fieldset_association_path(fieldset_association) %></td>
        <td><%= link_to 'Destroy', fieldset_association, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Fieldset Association', new_fieldset_association_path %>
