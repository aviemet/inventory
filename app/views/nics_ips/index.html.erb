<p id="notice"><%= notice %></p>

<h1>Nics Ips</h1>

<table>
  <thead>
    <tr>
      <th>Nic</th>
      <th>Ip</th>
      <th>Active</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @nics_ips.each do |nics_ip| %>
      <tr>
        <td><%= nics_ip.nic_id %></td>
        <td><%= nics_ip.ip_id %></td>
        <td><%= nics_ip.active %></td>
        <td><%= link_to 'Show', nics_ip %></td>
        <td><%= link_to 'Edit', edit_nics_ip_path(nics_ip) %></td>
        <td><%= link_to 'Destroy', nics_ip, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Nics Ip', new_nics_ip_path %>
