

<h1>Ipv4 Addresses</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @ipv4_addresses.each do |ipv4_address| %>
      <tr>
        <td><%= ipv4_address.address %></td>
        <td><%= link_to 'Show', ipv4_address %></td>
        <td><%= link_to 'Edit', edit_ipv4_address_path(ipv4_address) %></td>
        <td><%= link_to 'Destroy', ipv4_address, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ipv4 Address', new_ipv4_address_path %>
