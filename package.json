    "dinero.js": "^1.9.1",
    "immutable": "^5.0.3",
    "ip": "^2.0.1",
    "ip-address": "^10.0.1",
    "jsdom": "^25.0.1",
    "lodash": "^4.17.21",
    "lowlight": "^3.2.0",
    "normalize-url": "^8.0.1",
    "pluralize": "^8.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-html-props": "^2.0.9",
    "react-icons": "^5.4.0",
